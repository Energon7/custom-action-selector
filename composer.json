{
    "name": "energon7/custom-action-selector",
    "description": "This package allows you to change the action select box to a button view.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Energon7\\CustomActionSelector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Energon7\\CustomActionSelector\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
